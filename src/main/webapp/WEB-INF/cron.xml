<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
	<cron>
		<url>/notify</url>
		<description>Notifies users about new resources</description>
		<schedule>every 1 hours</schedule>
	</cron>

	<cron>
		<url>/heiko/tuesday</url>
		<description>Notifies Heiko about important stuff!</description>
		<schedule>every tuesday 12:00</schedule>
		<timezone>Europe/Berlin</timezone>
	</cron>
	<cron>
		<url>/heiko/today</url>
		<description>Notifies Heiko about important stuff!</description>
		<schedule>every wednesday 12:00</schedule>
		<timezone>Europe/Berlin</timezone>
	</cron>
</cronentries>
